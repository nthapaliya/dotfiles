# alias rubo='rubocop -R -D '
alias rubo="git status --porcelain | egrep '((rb)|(rake))' | awk '{print \$2}' | grep -v schema.rb | xargs rubocop -a -R -D"
alias rubox='rubocop -a -R -D '
alias al="ag --files-with-matches --smart-case"
