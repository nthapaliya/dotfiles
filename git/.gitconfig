[push]
	default = simple
[core]
	ignorecase = false
	excludesfile = /Users/niraj/.gitignore_global
	quotepath = false
[alias]
	up = "!git remote update -p; git merge --ff-only @{u}"
	ctags = !.git/hooks/ctags-run
